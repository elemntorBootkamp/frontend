    "babel-jest": "^29.5.0",
    "cypress": "^12.13.0",
    "dotenv": "^16.1.2",
    "eslint": "^8.42.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
