    "eslint-plugin-storybook": "^0.6.12",
    "jest": "^29.5.0",
    "jest-puppeteer": "^9.0.0",
    "prop-types": "^15.8.1",
    "puppeteer": "^20.5.0",
    "storybook": "^7.0.18",
    "vite": "^4.3.2"
  },
   "lint-staged": {
    "**/*.{js,jsx}": [
      "eslint . --fix"
    ]
